COMPOSE_PROJECT_NAME=raphtory
ZOOKEEPER=zooKeeper:2181
UPDATES_FREQ=1000
PARTITION_MIN=1
ROUTER_MIN=1
JAVA_OPTS=-XX:+UseConcMarkSweepGC -XX:+DisableExplicitGC -XX:+UseParNewGC -Xms40g -Xmx40g -XX:NewRatio=3
ADD_ONLY_VERTEX=false
ADD_ONLY_EDGE=false
PROMETHEUS=true
STDOUT_LOG=true
LAMCLASS=com.raphtory.examples.bitcoin.actors.BitcoinLiveAnalysisManager
ROUTERCLASS=com.raphtory.examples.bitcoin.actors.BitcoinRaphtoryRouter
UPDATERCLASS=com.raphtory.examples.bitcoin.actors.BitcoinExampleSpout
PARTITIONINGCLASS=com.raphtory.examples.partitioning.LinearGreedy
