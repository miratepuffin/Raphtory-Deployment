COMPOSE_PROJECT_NAME=raphtory
ZOOKEEPER=zooKeeper:2181
UPDATES_FREQ=1000
PARTITION_MIN=1
ROUTER_MIN=1
JAVA_OPTS=-XX:+UseConcMarkSweepGC -XX:+DisableExplicitGC -XX:+UseParNewGC -Xms20g -Xmx20g -XX:NewRatio=3
ADD_ONLY_VERTEX=false
ADD_ONLY_EDGE=false
PROMETHEUS=true
STDOUT_LOG=true
LAMCLASS=com.raphtory.examples.random.actors.RandomLAM
ROUTERCLASS=com.raphtory.examples.random.actors.RandomRouter
UPDATERCLASS=com.raphtory.examples.random.actors.RandomSpout
PARTITIONINGCLASS=com.raphtory.examples.partitioning.LinearGreedy
